<nav>
  {% set navPages = collections.all | eleventyNavigation %}
  <ul class="nav-container">
    {%- for entry in navPages %}
      <li>
        <a href="{{ entry.url | url }}"{% if entry.url == page.url %} data-current="current item" aria-current="page"{% endif %}>{{ entry.title }}</a>
      </li>
    {%- endfor %}
  </ul>
</nav>
